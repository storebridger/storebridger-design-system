{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "yarn run start:prod",
    "dev": "vite",
    "build": "CI=false yarn run clean && vite build",
    "serve": "vite preview",
    "clean": "rm -rf ./build",
    "test": "",
    "eject": "",
    "start:prod": "serve -s build",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "lint": "yarn run eslint -c eslint.config.cjs ./src",
    "lint:fix": "yarn run eslint -c eslint.config.cjs --fix ./src",
    "lint:css": "stylelint src/**/*.tsx --fix && stylelint src/**/*.ts --fix",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
    "prettify": "prettier --write src/**/*.tsx",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "uploadJsonFiles": "ts-node ./src/assets/uploadJson.ts",
    "uploadImagefiles": "ts-node ./src/assets/uploadImage.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": "10.4.0",
    "node": "20.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "dependencies": {
    "@casl/ability": "6.5.0",
